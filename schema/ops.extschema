									"type": "string",
									"enum": [
	                  "set",
	                  [
	                    "none",
	                    "cos",
											"dscp"
	                  ]
	                ],
									"description" : {
										"paragraphs": [
											[
												"Specifies the global default QoS Trust Mode. A value of 'none' will",
                        "not trust any packet code point fields, and remark all of them to 0.",
                        "A value of 'cos' will trust the 802.1Q priority code point and",
                        "preserve DSCP or IP-ToS. A value of 'dscp' will trust DSCP, and",
                        "optionally remark the 802.1Q priority code point to match."
											]
										]
									},
									"default": "none"
								},
