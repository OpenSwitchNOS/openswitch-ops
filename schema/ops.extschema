              "lldp_table_deletes" : {
                "type": "integer",
                "minInteger": 0,
                "group": ["LLDP Statistics"],
                "description": {
                  "paragraphs": [
                    [
                      "Number of neighbor entries deleted from LLDP neighbor table."
                    ]
                  ]
                },
                "default": 0
              },
              "lldp_table_drops" : {
                "type": "integer",
                "minInteger": 0,
                "group": ["LLDP Statistics"],
                "description": {
                  "paragraphs": [
                    [
                      "Number of neighbor entries that could not be added to LLDP",
                      "neighbor table and dropped."
                    ]
