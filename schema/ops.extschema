                    ]
                  ]
                }
              }
						}
					}
				},
				"bufmon_config": {
					"category": "configuration",
					"group": ["Bufmon configuration"],
					"type": {
						"map": {
							"enabled" : {
                "type": "boolean",
                "description" : {
                  "paragraphs": [
                    [
                      "Specifies whether the bufmon feature is enabled on the system."
                    ]
                  ]
                },
								"default": false
              },
							"counters_mode": {
								"period" : {
	                "type": "string",
									"enum": [
	                  "set",
	                  [
	                    "peak",
	                    "current"
	                  ]
									],
	                "description" : {
	                  "paragraphs": [
	                    [
	                      "Specifies whether counters should report their current values or peak values",
                        "since last collection."
	                    ]
	                  ]
	                },
									"default": "current"
	              },
								"periodic_collection_enabled" : {
	                "type": "boolean",
	                "description" : {
	                  "paragraphs": [
	                    [
	                      "Specifies whether periodic collection of the counters is enabled."
	                    ]
	                  ]
	                },
									"default": false
	              },
								"collection_period" : {
	                "type": "integer",
	                "minInteger": 1,
	                "description" : {
	                  "paragraphs": [
	                    [
	                      "Specifies the period of collection in seconds."
	                    ]
	                  ]
	                },
									"default": 5
	              },
								"threshold_trigger_collection_enabled" : {
	                "type": "boolean",
	                "description" : {
	                  "paragraphs": [
	                    [
	                      "Specifies whether counters should be collected immediately",
                        "when one of the thresholds is crossed."
	                    ]
	                  ]
	                },
									"default": true
	              },
								"threshold_trigger_rate_limit" : {
									"type": "integer",
									"minInteger": 1,
									"maxInteger": 600,
									"description" : {
										"paragraphs": [
											[
												"Specifies maximum number of trigger generated reports per minute.",
                        "The limit will be averaged and imposed on a per second basis.",
                         "For example, value of 600 will allow report every 100ms."
											 ]
										]
									},
									"default": 60
								},
								"snapshot_on_threshold_trigger" : {
	                "type": "boolean",
	                "description" : {
	                  "paragraphs": [
	                    [
	                      "Specifies whether counters should be frozen when one of the thresholds is crossed."
	                    ]
	                  ]
	                },
									"default": true
	              }
							}
						}
					}
				},
        "bufmon_info": {
          "category": "status",
					"group": ["information"],
          "type": {
            "map": {
							"cap_mode_peak" : {
								"type": "boolean",
								"description" : {
									"paragraphs": [
										[
											"Specifies whether the system is capable of collecting peak values of",
                      "the counters."
										]
									]
								},
								"default": false
							},
							"cap_mode_current" : {
								"type": "boolean",
								"description" : {
									"paragraphs": [
										[
											"Specifies whether the system is capable of collecting current values of",
                      "the counters."
										]
									]
								},
								"default": false
							},
							"cap_snapshot_on_threshold_trigger" : {
								"type": "boolean",
								"description" : {
									"paragraphs": [
										[
											"Specifies whether the system is capable of freezing counters values on",
                      "threshold crossing."
										]
									]
								},
								"default": false
							},
							"cap_threshold_trigger_collection" : {
								"type": "boolean",
								"description" : {
									"paragraphs": [
										[
